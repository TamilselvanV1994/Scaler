let array2D =[
    [1,2,3],
    [4,5,6]];
//console.log(array2D[0].length*array2D.length);

// Print the row-wise sum

function printRowWiseSum(array){
    let n=array2D.length;
let m=array2D[0].length;
for(let i=0; i<n;i++){
    sum=0;
    for(let j=0; j<m;j++){
        sum+=array[i][j];
    }
    console.log(sum);
 }
}

printRowWiseSum(array2D);
